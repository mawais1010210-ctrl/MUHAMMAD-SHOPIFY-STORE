<script>
  document.addEventListener('DOMContentLoaded', function () {
    const tabs = document.querySelectorAll('.custom_biling_tabs');
    const faqSections = document.querySelectorAll('.custm_wrper_faq-contnt');
    const searchInput = document.querySelector('#faq-search-mobile');

    // âœ… Default: pehla tab active, sirf uski section show
    if (tabs.length > 0 && faqSections.length > 0) {
      tabs.forEach((t, i) => {
        t.classList.remove('active');
        faqSections[i].style.display = 'none'; // saare hide
      });

      // Pehla tab aur uski section show
      tabs[0].classList.add('active');
      faqSections[0].style.display = 'block';
      resetSection(faqSections[0]);
    }

    // Tab click
    tabs.forEach((tab, index) => {
      tab.addEventListener('click', () => {
        tabs.forEach((t) => t.classList.remove('active'));
        tab.classList.add('active');

        faqSections.forEach((sec) => {
          sec.style.display = 'none';
          resetSection(sec);
        });

        faqSections[index].style.display = 'block';

        // Reset search input
        if (searchInput) searchInput.value = '';
      });
    });

    // Accordion
    document.querySelectorAll('.custm_wrprers_fq-question').forEach((question) => {
      question.addEventListener('click', function () {
        const answer = this.nextElementSibling;
        const openQuestion = document.querySelector('.custm_wrprers_fq-question.open');
        if (openQuestion && openQuestion !== this) {
          openQuestion.classList.remove('open');
          slideUp(openQuestion.nextElementSibling);
        }
        this.classList.toggle('open');
        if (this.classList.contains('open')) slideDown(answer);
        else slideUp(answer);
      });
    });

    // Search filter (all tabs)
    if (searchInput) {
      searchInput.addEventListener('input', function () {
        const value = this.value.toLowerCase();

        faqSections.forEach((section) => {
          let anyMatch = false;

          section.querySelectorAll('.custm_wprfq-item').forEach((item) => {
            const qText = item.querySelector('.custm_wrprers_fq-question p').textContent.toLowerCase();
            const aText = item.querySelector('.custm_ansr_wrapfa-anwer').textContent.toLowerCase();

            if (qText.includes(value) || aText.includes(value)) {
              item.style.display = 'block';
              anyMatch = true;
            } else {
              item.style.display = 'none';
              item.querySelector('.custm_wrprers_fq-question').classList.remove('open');
              const ans = item.querySelector('.custm_ansr_wrapfa-anwer');
              ans.style.display = 'none';
              ans.style.height = '';
            }
          });

          // Show section if it has any matching items
          if (anyMatch) {
            section.style.display = 'block';
          } else {
            section.style.display = 'none';
          }
        });
      });
    }

    // Utility: reset a section
    function resetSection(section) {
      section.querySelectorAll('.custm_wrprers_fq-question').forEach((q) => q.classList.remove('open'));
      section.querySelectorAll('.custm_ansr_wrapfa-anwer').forEach((ans) => {
        ans.style.display = 'none';
        ans.style.height = '';
      });
    }

    // Slide functions
    function slideUp(el) {
      el.style.height = el.scrollHeight + 'px';
      requestAnimationFrame(() => {
        el.style.transition = 'height 0.4s ease';
        el.style.height = '0px';
      });
      el.addEventListener('transitionend', function handler() {
        el.style.display = 'none';
        el.style.height = '';
        el.style.transition = '';
        el.removeEventListener('transitionend', handler);
      });
    }

    function slideDown(el) {
      el.style.display = 'block';
      let h = el.scrollHeight + 'px';
      el.style.height = '0px';
      requestAnimationFrame(() => {
        el.style.transition = 'height 0.4s ease';
        el.style.height = h;
      });
      el.addEventListener('transitionend', function handler() {
        el.style.height = '';
        el.style.transition = '';
        el.removeEventListener('transitionend', handler);
      });
    }
  });
</script>
