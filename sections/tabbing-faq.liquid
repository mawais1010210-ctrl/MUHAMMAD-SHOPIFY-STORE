<style>
  * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
      font-family: sans-serif;
      list-style: none;
  }

  .tabing_faq-sec {
      padding: 40px 0;
  }

  .tabing_faq-container {
      max-width: 1337px;
      margin: 0 auto;
      width: 100%;
      padding: 0 50px;
  }

  .customers_title-heading {
      color: #454545;
      margin: 0;
      font-size: 56px;
      letter-spacing: 0 !important;
      line-height: 100%;
      text-align: center;
  }

  .customer-description-para p {
      color: #454545;
      text-align: center;
      font-size: 14px;
      letter-spacing: 0 !important;
      line-height: 100%;
      padding: 20px 0 0 0;
      width: 617px;
      margin: 0 auto;
  }

  .customer-form-search-frequently-img {
      position: relative;
      margin: 50px auto;
      max-width: 1084px;
      padding: 0 40px;
  }

  .custom_serch_inp input[type="search"] {
      border-radius: 100px;
      width: 100%;
      padding: 20px;
      outline: none;
      box-shadow: none;
      border: 1px solid #D9D9D9;
      font-size: 16px;
  }

  .customer-search-image img {
      max-width: 44px;
  }

  .customer-search-image {
      position: absolute;
      top: 8px;
      right: 52px;
  }

  .mobile-image {
      display: none;
  }

  .customer-tabbing-equipment {
      display: flex;
      align-items: center;
      justify-content: space-between;
      max-width: 1084px;
      margin: 0 auto;
      padding: 0 40px;
  }

  .custom_biling_tabs {
      width: 15.5%;
      border-radius: 16.44px;
      text-align: center;
      cursor: pointer;
      padding: 29px 15px;
      border: 1px solid #D9D9D9;
      min-height: 160px;

      transition: 0.3s;
  }

  .custom_biling_tabs .desktop-image,
  .custom_biling_tabs .custm_main_title {
      filter: grayscale(100%);
      opacity: 0.7;
  }

  /* Jab koi tab active ho jaye */
  .custom_biling_tabs.active .desktop-image,
  .custom_biling_tabs.active .custm_main_title {
      filter: none !important;
      opacity: 1 !important;
  }

  .desktop-image {
      max-width: 53px;
      margin: 0 auto;
      line-height: 0;
  }

  .customer-main-title {
      padding-top: 10px;
  }

  .custm_main_title {
      font-size: 14px;
      line-height: 100%;
      margin: 0;
      letter-spacing: 0 !important;
      color: #848484;
      font-weight: 400;
  }

  .customer-tab-faq-wrapper {
      margin: 30px 0 0 0;
  }

  .custm_ansr_wrapfa-anwer p {
      margin: 0;
      color: #454545;
      font-size: 14px;
      letter-spacing: 0 !important;
      line-height: 1.3;
  }


  .custom_biling_tabs.active {
      border: 2px solid #CEF34E;
      background: #F1FBCC;
  }

  /* FAQ Sections */
  .faq-section {
      display: none;
  }

  .faq-section.active {
      display: block;
  }

  /* FAQ Accordion */

  .custm_wprfq-item {
      background: #f9f6f0;
      margin-bottom: 10px;
      border-radius: 20px;
      padding: 0;
      cursor: pointer;
  }

  .custm_wrprers_fq-question {
      position: relative;
      padding-right: 25px;
      cursor: pointer;
      font-size: 16px;
      transition: all 0.5s ease-in-out !important;
      padding-bottom: 20px;
      padding-top: 20px;
      padding-left: 20px;
  }

  .custm_wrprers_fq-question::after {
      content: "+";
      position: absolute;
      right: 20px;
      bottom: 13px;
      font-size: 18px;
      color: #555;
      transition: transform 0.3s ease;
      line-height: 26px;
  }

  .custm_wrprers_fq-question.open {
      color: #70BC0E;
  }

  .custm_wrprers_fq-question p {
      margin: 0;
      color: #454545;
      font-size: 14px;
      line-height: 100%;
      letter-spacing: 0 !important;
      transition: color 0.3s ease;
  }

  .custm_wrprers_fq-question.open::after {
      content: "-";
      transform: rotate(180deg);
      color: #000;
  }

  .custm_ansr_wrapfa-anwer {
      display: none;
      padding-left: 20px;
      padding-right: 20px;
      padding-bottom: 0;
      overflow: hidden;
  }

  .custom_biling_tabs:hover {
      background: #eeeeee;
  }

  @media (max-width: 767px) {

  .tabing_faq-container {
      padding: 0 20px;
  }

  .customers_title-heading {
      font-size: 32px;
  }

  .customer-description-para p {
      width: 100%;
      font-size: 13px;
  }

  /* Search box center and smaller padding */
  .customer-form-search-frequently-img {
      padding: 0 15px;
      margin: 30px auto;
  }

  .custom_serch_inp input[type="search"] {
      padding: 14px 18px;
      font-size: 14px;
  }

  .customer-search-image {
      right: 22px;
      top: 6px;
  }

  /* TABS SCROLLABLE IN MOBILE */
  .customer-tabbing-equipment {
      display: flex;
      overflow-x: auto;
      gap: 16px;
      padding: 0 10px;
      justify-content: flex-start;
  }

  .customer-tabbing-equipment::-webkit-scrollbar {
      display: none;
  }

  .custom_biling_tabs {
      min-width: 140px;
      width: auto;
      min-height: 120px;
      padding: 20px 10px;
      flex-shrink: 0;
  }

  .custm_main_title {
      font-size: 12px;
  }

  /* FAQ WRAPPER */
  .customer-tab-faq-wrapper {
      margin-top: 20px;
  }

  .custm_wrprers_fq-question {
      font-size: 14px;
      padding: 15px 15px;
  }

  .custm_ansr_wrapfa-anwer {
      padding: 0 15px 10px 15px;
  }

}

